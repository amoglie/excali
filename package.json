  "private": true,
  "dependencies": {
    "@excalidraw/excalidraw": "^0.17.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "serve": "^14.2.3"
  },
"scripts": {
